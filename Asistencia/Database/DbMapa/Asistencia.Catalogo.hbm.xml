<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Asistencia" namespace="Asistencia.DbDominio" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Catalogo" table="catalogos" schema="dbo" dynamic-insert="true" dynamic-update="true" select-before-update="true">
    <id name="CveCatalogos" type="Int32">
      <column name="cve_catalogos" not-null="true" precision="10" scale="0" sql-type="int" />
      <generator class="assigned" />
    </id>
    <property name="Descripcion" type="String">
      <column name="descripcion" not-null="true" length="50" sql-type="varchar" />
    </property>
  </class>
</hibernate-mapping>